{
  "plugins": ["prettier-plugin-tailwindcss"],
  "singleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "htmlWhitespaceSensitivity": "ignore",
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "singleAttributePerLine": true,
  "endOfLine": "auto"
}
